{
  "name": "salesforce-explort",
  "version": "0.1.0",
  "description": "An explorer for Salesforce, like workbench but on your desktop",
  "author": "Stan Lemon <stosh1985@gmail.com>",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=development electron main.js",
    "test": "mocha",
    "build": "babel src -d dist",
    "lint": "eslint src",
    "pack": "electron-packager . RetroJs --overwrite --platform=darwin --arch=x64 --version=0.37.1 --asar=false --prune --ignore='src' --ignore='release' --ignore='test' --out=release"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-preset-react": "^6.11.1",
    "electron-packager": "^7.3.0",
    "electron-prebuilt-compile": "^1.2.7",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "react-addons-test-utils": "^15.2.1",
    "watch-glob": "^0.1.3"
  },
  "dependencies": {
    "@salesforce-ux/design-system": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "dotenv": "^2.0.0",
    "jsforce": "^1.7.0",
    "localforage": "^1.4.2",
    "lodash": "^4.13.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-persist": "^3.2.2"
  }
}
